{
  "name": "weather-app",
  "private": true,
  "type": "module",
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=20.18.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV='development' NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "LOG='standard' NODE_ENV='test' vitest --coverage --reporter=verbose",
    "test:visual": "LOG='standard' NODE_ENV='test' vitest --coverage --reporter=verbose --ui",
    "test:visual:css": "LOG='standard' NODE_ENV='test' vitest-preview",
    "audit": "better-npm-audit audit",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "translate:generate": "i18next 'app/**/*.{ts,tsx}' 'src/**/*.{ts,tsx}' -o 'src/i18n/locales/$LOCALE/$NAMESPACE.json' -c 'src/i18n/i18next-parser.config.ts'"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.5",
    "@emotion/react": "^11.13.5",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.13.5",
