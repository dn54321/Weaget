{
  "name": "weather-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=24.13.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV='development' NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "eslint --fix *.{ts,tsx}",
    "lint:type": "tsc --noEmit",
    "lint:all": "eslint --fix *.{ts,tsx} && tsc --noEmit",
    "test": "LOG='standard' NODE_ENV='test' vitest --coverage --reporter=verbose",
    "test:dashboard": "LOG='standard' NODE_ENV='test' vitest --coverage --reporter=verbose --ui",
    "test:visualiser": "LOG='standard' NODE_ENV='test' vitest-preview",
    "audit": "better-npm-audit audit",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "translate:generate": "i18next 'app/**/*.{ts,tsx}' 'src/**/*.{ts,tsx}' -o 'src/i18n/locales/$LOCALE/$NAMESPACE.json' -c 'src/i18n/i18next-parser.config.ts'"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.14.1",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/material-nextjs": "^7.3.7",
    "@mui/system": "^7.3.7",
    "@next/bundle-analyzer": "^16.1.5",
    "@storybook/nextjs-vite": "^10.2.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/css-mediaquery": "^0.1.4",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "css-mediaquery": "^0.1.2",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "luxon": "^3.7.2",
    "next": "^16.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.4",
    "recharts": "2.14.1",
    "three-dots": "^0.3.2",
    "throttle-debounce": "^5.0.2",
    "use-sync-external-store": "^1.6.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@dword-design/eslint-plugin-import-alias": "^5.1.2",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^9.9.0",
    "@next/eslint-plugin-next": "^15.5.10",
    "@storybook/react": "^10.2.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.7",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/throttle-debounce": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-sort-imports-requires": "^2.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "jsdom": "^25.0.1",
    "msw": "^2.12.7",
    "storybook": "^10.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18",
    "vitest-preview": "^0.0.3"
  }
}
